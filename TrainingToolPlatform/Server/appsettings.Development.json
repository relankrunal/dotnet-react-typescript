{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Origins": [ "http://localhost:3000" ],
    "Methods": [ "GET", "POST", "PUT", "DELETE" ],
    "Headers": [ "Content-Type", "Authorization" ]
  },
  "XSRF": {
    "Enabled": true
  }
}